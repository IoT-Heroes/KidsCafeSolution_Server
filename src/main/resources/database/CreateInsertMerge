ALTER TABLE ZONE
	ADD CONSTRAINT FK_CODE_TO_ZONE
	 -- 코드 -> 놀이구역
		FOREIGN KEY (
			CODE_ID -- ID2
		)
		REFERENCES CODE ( -- 코드
			ID -- ID
		);

-- 코드 -> 놀이구역
COMMENT ON CONSTRAINT FK_CODE_TO_ZONE ON ZONE IS '코드 -> 놀이구역';

-- 방문이력별이용통계
ALTER TABLE ZONE_USING_ONE_DAY_STATISTICS
	ADD CONSTRAINT FK_CAFE_VISITING_RECORD_TO_ZONE_USING_ONE_DAY_STATISTICS
	 -- 키즈카페방문이력 -> 방문이력별이용통계
		FOREIGN KEY (
			CHILD_ID,   -- 자녀ID
			START_DATE  -- 입장시간
		)
		REFERENCES CAFE_VISITING_RECORD ( -- 키즈카페방문이력
			CHILD_ID,   -- 자녀ID
			START_DATE  -- 입장시간
		);

-- 키즈카페방문이력 -> 방문이력별이용통계
COMMENT ON CONSTRAINT FK_CAFE_VISITING_RECORD_TO_ZONE_USING_ONE_DAY_STATISTICS ON ZONE_USING_ONE_DAY_STATISTICS IS '키즈카페방문이력 -> 방문이력별이용통계';

-- 방문이력별이용통계
ALTER TABLE ZONE_USING_ONE_DAY_STATISTICS
	ADD CONSTRAINT FK_ZONE_TO_ZONE_USING_ONE_DAY_STATISTICS
	 -- 놀이구역 -> 방문이력별이용통계
		FOREIGN KEY (
			ZONE_ID -- 놀이구역ID
		)
		REFERENCES ZONE ( -- 놀이구역
			ID -- ID
		);

-- 놀이구역 -> 방문이력별이용통계
COMMENT ON CONSTRAINT FK_ZONE_TO_ZONE_USING_ONE_DAY_STATISTICS ON ZONE_USING_ONE_DAY_STATISTICS IS '놀이구역 -> 방문이력별이용통계';

-- 자녀활동량일통계
ALTER TABLE CHILD_ACTIVITY_DAY_STATISTICS
	ADD CONSTRAINT FK_K_CHILD_TO_CHILD_ACTIVITY_DAY_STATISTICS
	 -- 자녀 -> 자녀활동량일통계
		FOREIGN KEY (
			CHILD_ID -- 자녀ID
		)
		REFERENCES K_CHILD ( -- 자녀
			ID -- ID
		);

-- 자녀 -> 자녀활동량일통계
COMMENT ON CONSTRAINT FK_K_CHILD_TO_CHILD_ACTIVITY_DAY_STATISTICS ON CHILD_ACTIVITY_DAY_STATISTICS IS '자녀 -> 자녀활동량일통계';

-- 자녀놀이구역방문일통계
ALTER TABLE CHILD_ZONE_VISITING_DAY_STATISTICS
