-- 놀이구역쾌적도시통계
CREATE TABLE ZONE_AIR_HOUR_STATISTICS
(
	ZONE_ID             varchar(50) NOT NULL, -- 놀이구역ID
	DATE                timestamp   NOT NULL, -- 일시
	AVERAGE_CO2         integer     NULL,     -- 평균이산화탄소
	AVERAGE_TEMPERATURE integer     NULL,     -- 평균온도
	AVERAGE_HUMIDITY    integer     NULL,     -- 평균습도
	AVERAGE_DUST        integer     NULL      -- 평균먼지
);

-- 놀이구역쾌적도시통계
COMMENT ON TABLE ZONE_AIR_HOUR_STATISTICS IS '놀이구역쾌적도시통계';

-- 놀이구역ID
COMMENT ON COLUMN ZONE_AIR_HOUR_STATISTICS.ZONE_ID IS '놀이구역ID';

-- 일시
COMMENT ON COLUMN ZONE_AIR_HOUR_STATISTICS.DATE IS '일시';

-- 평균이산화탄소
COMMENT ON COLUMN ZONE_AIR_HOUR_STATISTICS.AVERAGE_CO2 IS '평균이산화탄소';

-- 평균온도
COMMENT ON COLUMN ZONE_AIR_HOUR_STATISTICS.AVERAGE_TEMPERATURE IS '평균온도';

-- 평균습도
COMMENT ON COLUMN ZONE_AIR_HOUR_STATISTICS.AVERAGE_HUMIDITY IS '평균습도';

-- 평균먼지
COMMENT ON COLUMN ZONE_AIR_HOUR_STATISTICS.AVERAGE_DUST IS '평균먼지';

-- 놀이구역쾌적도시통계 기본키
CREATE UNIQUE INDEX PK_ZONE_AIR_HOUR_STATISTICS
	ON ZONE_AIR_HOUR_STATISTICS
	( -- 놀이구역쾌적도시통계
		ZONE_ID ASC, -- 놀이구역ID
		DATE ASC -- 일시
	)
;
-- 놀이구역쾌적도시통계
ALTER TABLE ZONE_AIR_HOUR_STATISTICS
	ADD CONSTRAINT PK_ZONE_AIR_HOUR_STATISTICS
		 -- 놀이구역쾌적도시통계 기본키
	PRIMARY KEY 
	USING INDEX PK_ZONE_AIR_HOUR_STATISTICS;

-- 놀이구역쾌적도시통계 기본키
COMMENT ON CONSTRAINT PK_ZONE_AIR_HOUR_STATISTICS ON ZONE_AIR_HOUR_STATISTICS IS '놀이구역쾌적도시통계 기본키';

-- 코드그룹
CREATE TABLE CODE_GROUP
(
	ID   varchar(50) NOT NULL, -- ID
	NAME varchar(50) NULL      -- 코드그룹명
);

-- 코드그룹
COMMENT ON TABLE CODE_GROUP IS '코드그룹';

-- ID
COMMENT ON COLUMN CODE_GROUP.ID IS 'ID';

-- 코드그룹명
COMMENT ON COLUMN CODE_GROUP.NAME IS '코드그룹명';

-- 코드그룹 기본키
CREATE UNIQUE INDEX PK_CODE_GROUP
	ON CODE_GROUP
	( -- 코드그룹
		ID ASC -- ID
	)
;
-- 코드그룹
ALTER TABLE CODE_GROUP
	ADD CONSTRAINT PK_CODE_GROUP
		 -- 코드그룹 기본키
	PRIMARY KEY 
	USING INDEX PK_CODE_GROUP;

-- 코드그룹 기본키
COMMENT ON CONSTRAINT PK_CODE_GROUP ON CODE_GROUP IS '코드그룹 기본키';

-- 코드
CREATE TABLE CODE
(
	ID            varchar(50) NOT NULL, -- ID
	CODE_GROUP_ID varchar(50) NULL,     -- 코드그룹ID
	CODE_NAME     varchar(50) NULL      -- 코드상태명
);

-- 코드
COMMENT ON TABLE CODE IS '코드';

-- ID
COMMENT ON COLUMN CODE.ID IS 'ID';

-- 코드그룹ID
COMMENT ON COLUMN CODE.CODE_GROUP_ID IS '코드그룹ID';

-- 코드상태명
COMMENT ON COLUMN CODE.CODE_NAME IS '코드상태명';

-- 코드 기본키
CREATE UNIQUE INDEX PK_CODE
	ON CODE
	( -- 코드
		ID ASC -- ID
	)
;
-- 코드
ALTER TABLE CODE
	ADD CONSTRAINT PK_CODE
		 -- 코드 기본키
	PRIMARY KEY 
	USING INDEX PK_CODE;

-- 코드 기본키
COMMENT ON CONSTRAINT PK_CODE ON CODE IS '코드 기본키';

-- 키즈카페방문이력
ALTER TABLE CAFE_VISITING_RECORD
	ADD CONSTRAINT FK_K_CHILD_TO_CAFE_VISITING_RECORD
	 -- 자녀 -> 키즈카페방문이력
		FOREIGN KEY (
			CHILD_ID -- 자녀ID
		)
		REFERENCES K_CHILD ( -- 자녀
			ID -- ID
		);

-- 자녀 -> 키즈카페방문이력
COMMENT ON CONSTRAINT FK_K_CHILD_TO_CAFE_VISITING_RECORD ON CAFE_VISITING_RECORD IS '자녀 -> 키즈카페방문이력';

-- 키즈카페방문이력
ALTER TABLE CAFE_VISITING_RECORD
	ADD CONSTRAINT FK_BAND_DEVICE_TO_CAFE_VISITING_RECORD
	 -- 밴드디바이스 -> 키즈카페방문이력
		FOREIGN KEY (
			BAND_DEVICE_ID -- 밴드디바이스ID
		)
		REFERENCES BAND_DEVICE ( -- 밴드디바이스
			ID -- ID
		);

-- 밴드디바이스 -> 키즈카페방문이력
COMMENT ON CONSTRAINT FK_BAND_DEVICE_TO_CAFE_VISITING_RECORD ON CAFE_VISITING_RECORD IS '밴드디바이스 -> 키즈카페방문이력';

-- 자녀
ALTER TABLE K_CHILD
	ADD CONSTRAINT FK_K_USER_TO_K_CHILD
	 -- 회원 -> 자녀
		FOREIGN KEY (
			USER_ID -- 회원ID
		)
		REFERENCES K_USER ( -- 회원
			ID -- ID
		);

-- 회원 -> 자녀
COMMENT ON CONSTRAINT FK_K_USER_TO_K_CHILD ON K_CHILD IS '회원 -> 자녀';

-- 자녀놀이구역방문시통계
ALTER TABLE CHILD_ZONE_VISITING_HOUR_STATISTICS
	ADD CONSTRAINT FK_ZONE_TO_CHILD_ZONE_VISITING_HOUR_STATISTICS
	 -- 놀이구역 -> 자녀놀이구역방문시통계
		FOREIGN KEY (
			ZONE_ID -- 놀이구역ID
		)
		REFERENCES ZONE ( -- 놀이구역
			ID -- ID
		);

-- 놀이구역 -> 자녀놀이구역방문시통계
COMMENT ON CONSTRAINT FK_ZONE_TO_CHILD_ZONE_VISITING_HOUR_STATISTICS ON CHILD_ZONE_VISITING_HOUR_STATISTICS IS '놀이구역 -> 자녀놀이구역방문시통계';

-- 자녀활동량시통계
ALTER TABLE CHILD_ACTIVITY_HOUR_STATISTICS
	ADD CONSTRAINT FK_K_CHILD_TO_CHILD_ACTIVITY_HOUR_STATISTICS
	 -- 자녀 -> 자녀활동량시통계
		FOREIGN KEY (
			CHILD_ID -- 자녀ID
		)
		REFERENCES K_CHILD ( -- 자녀
			ID -- ID
		);

-- 자녀 -> 자녀활동량시통계
COMMENT ON CONSTRAINT FK_K_CHILD_TO_CHILD_ACTIVITY_HOUR_STATISTICS ON CHILD_ACTIVITY_HOUR_STATISTICS IS '자녀 -> 자녀활동량시통계';

-- 밴드디바이스
ALTER TABLE BAND_DEVICE
	ADD CONSTRAINT FK_ZONE_TO_BAND_DEVICE2
	 -- 놀이구역 -> 밴드디바이스2
		FOREIGN KEY (
			CURRENT_LOCATION -- 현재위치
		)
		REFERENCES ZONE ( -- 놀이구역
			ID -- ID
		);

-- 놀이구역 -> 밴드디바이스2
COMMENT ON CONSTRAINT FK_ZONE_TO_BAND_DEVICE2 ON BAND_DEVICE IS '놀이구역 -> 밴드디바이스2';

-- 밴드디바이스
ALTER TABLE BAND_DEVICE
	ADD CONSTRAINT FK_ZONE_TO_BAND_DEVICE
	 -- 놀이구역 -> 밴드디바이스
		FOREIGN KEY (
			BEFORE_LOCATION -- 이전위치
		)
		REFERENCES ZONE ( -- 놀이구역
			ID -- ID
		);

-- 놀이구역 -> 밴드디바이스
COMMENT ON CONSTRAINT FK_ZONE_TO_BAND_DEVICE ON BAND_DEVICE IS '놀이구역 -> 밴드디바이스';

