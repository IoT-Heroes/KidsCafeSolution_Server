-- 최소방문횟수
COMMENT ON COLUMN ZONE_VISITING_HOUR_STATISTICS.MINIMUM IS '최소방문횟수';

-- 최대방문횟수
COMMENT ON COLUMN ZONE_VISITING_HOUR_STATISTICS.MAXIMUM IS '최대방문횟수';

-- 놀이구역방문시통계 기본키
CREATE UNIQUE INDEX PK_ZONE_VISITING_HOUR_STATISTICS
	ON ZONE_VISITING_HOUR_STATISTICS
	( -- 놀이구역방문시통계
		ZONE_ID ASC, -- 놀이구역ID
		AGE ASC, -- 나이
		DATE ASC -- 일시
	)
;
-- 놀이구역방문시통계
ALTER TABLE ZONE_VISITING_HOUR_STATISTICS
	ADD CONSTRAINT PK_ZONE_VISITING_HOUR_STATISTICS
