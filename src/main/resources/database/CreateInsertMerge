-- 코드
ALTER TABLE CODE
	ADD CONSTRAINT FK_CODE_GROUP_TO_CODE
	 -- 코드그룹 -> 코드
		FOREIGN KEY (
			CODE_GROUP_ID -- 코드그룹ID
		)
		REFERENCES CODE_GROUP ( -- 코드그룹
			ID -- ID
		);

-- 코드그룹 -> 코드
COMMENT ON CONSTRAINT FK_CODE_GROUP_TO_CODE ON CODE IS '코드그룹 -> 코드';

