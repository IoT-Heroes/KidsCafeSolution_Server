-- 최대매출
COMMENT ON COLUMN CAFE_INCOME_HOUR_STATISTICS.MAXIMUM IS '최대매출';

-- 최소매출
COMMENT ON COLUMN CAFE_INCOME_HOUR_STATISTICS.MINIMUM IS '최소매출';

-- 키즈카페매출시통계 기본키
CREATE UNIQUE INDEX PK_CAFE_INCOME_HOUR_STATISTICS
	ON CAFE_INCOME_HOUR_STATISTICS
	( -- 키즈카페매출시통계
		DATE ASC -- 일시
	)
;
-- 키즈카페매출시통계
ALTER TABLE CAFE_INCOME_HOUR_STATISTICS
	ADD CONSTRAINT PK_CAFE_INCOME_HOUR_STATISTICS
		 -- 키즈카페매출시통계 기본키
	PRIMARY KEY 
	USING INDEX PK_CAFE_INCOME_HOUR_STATISTICS;

-- 키즈카페매출시통계 기본키
COMMENT ON CONSTRAINT PK_CAFE_INCOME_HOUR_STATISTICS ON CAFE_INCOME_HOUR_STATISTICS IS '키즈카페매출시통계 기본키';

-- 연령별활동량시통계
CREATE TABLE AGE_ACTIVITY_HOUR_STATISTICS
(
	DATE    timestamp NOT NULL, -- 일시
	AGE     integer   NOT NULL, -- 연령
	AVERAGE integer   NULL,     -- 평균활동량
	MAXIMUM integer   NULL,     -- 최대활동량
	MINIMUM integer   NULL      -- 최소활동량
);

-- 연령별활동량시통계
COMMENT ON TABLE AGE_ACTIVITY_HOUR_STATISTICS IS '연령별활동량시통계';

-- 일시
COMMENT ON COLUMN AGE_ACTIVITY_HOUR_STATISTICS.DATE IS '일시';

-- 연령
COMMENT ON COLUMN AGE_ACTIVITY_HOUR_STATISTICS.AGE IS '연령';

-- 평균활동량
COMMENT ON COLUMN AGE_ACTIVITY_HOUR_STATISTICS.AVERAGE IS '평균활동량';

-- 최대활동량
COMMENT ON COLUMN AGE_ACTIVITY_HOUR_STATISTICS.MAXIMUM IS '최대활동량';

-- 최소활동량
COMMENT ON COLUMN AGE_ACTIVITY_HOUR_STATISTICS.MINIMUM IS '최소활동량';

-- 연령별활동량시통계 기본키
CREATE UNIQUE INDEX PK_AGE_ACTIVITY_HOUR_STATISTICS
	ON AGE_ACTIVITY_HOUR_STATISTICS
	( -- 연령별활동량시통계
		DATE ASC, -- 일시
		AGE ASC -- 연령
	)
;
-- 연령별활동량시통계
ALTER TABLE AGE_ACTIVITY_HOUR_STATISTICS
	ADD CONSTRAINT PK_AGE_ACTIVITY_HOUR_STATISTICS
		 -- 연령별활동량시통계 기본키
	PRIMARY KEY 
	USING INDEX PK_AGE_ACTIVITY_HOUR_STATISTICS;

-- 연령별활동량시통계 기본키
COMMENT ON CONSTRAINT PK_AGE_ACTIVITY_HOUR_STATISTICS ON AGE_ACTIVITY_HOUR_STATISTICS IS '연령별활동량시통계 기본키';

-- 키즈카페이용일통계
CREATE TABLE CAFE_USAGE_DAY_STATISTICS
(
	DATE    timestamp NOT NULL, -- 일시
	AGE     integer   NOT NULL, -- 연령
	AVERAGE integer   NULL,     -- 평균고객수
	MAXIMUM integer   NULL,     -- 최대고객수
	MINIMUM integer   NULL      -- 최소고객수
);

-- 키즈카페이용일통계
COMMENT ON TABLE CAFE_USAGE_DAY_STATISTICS IS '키즈카페이용일통계';

-- 일시
COMMENT ON COLUMN CAFE_USAGE_DAY_STATISTICS.DATE IS '일시';

-- 연령
COMMENT ON COLUMN CAFE_USAGE_DAY_STATISTICS.AGE IS '연령';

-- 평균고객수
COMMENT ON COLUMN CAFE_USAGE_DAY_STATISTICS.AVERAGE IS '평균고객수';

-- 최대고객수
COMMENT ON COLUMN CAFE_USAGE_DAY_STATISTICS.MAXIMUM IS '최대고객수';

-- 최소고객수
COMMENT ON COLUMN CAFE_USAGE_DAY_STATISTICS.MINIMUM IS '최소고객수';

-- 키즈카페이용일통계 기본키
CREATE UNIQUE INDEX PK_CAFE_USAGE_DAY_STATISTICS
	ON CAFE_USAGE_DAY_STATISTICS
	( -- 키즈카페이용일통계
		DATE ASC, -- 일시
		AGE ASC -- 연령
	)
;
-- 키즈카페이용일통계
ALTER TABLE CAFE_USAGE_DAY_STATISTICS
	ADD CONSTRAINT PK_CAFE_USAGE_DAY_STATISTICS
		 -- 키즈카페이용일통계 기본키
	PRIMARY KEY 
	USING INDEX PK_CAFE_USAGE_DAY_STATISTICS;

-- 키즈카페이용일통계 기본키
COMMENT ON CONSTRAINT PK_CAFE_USAGE_DAY_STATISTICS ON CAFE_USAGE_DAY_STATISTICS IS '키즈카페이용일통계 기본키';

-- 자녀놀이구역방문일통계
CREATE TABLE CHILD_ZONE_VISITING_DAY_STATISTICS
(
	ZONE_ID varchar(50) NOT NULL, -- 놀이구역ID
	DATE    timestamp   NOT NULL, -- 일시
	COUNT   integer     NULL      -- 평균방문횟수
);

-- 자녀놀이구역방문일통계
COMMENT ON TABLE CHILD_ZONE_VISITING_DAY_STATISTICS IS '자녀놀이구역방문일통계';

-- 놀이구역ID
COMMENT ON COLUMN CHILD_ZONE_VISITING_DAY_STATISTICS.ZONE_ID IS '놀이구역ID';

-- 일시
COMMENT ON COLUMN CHILD_ZONE_VISITING_DAY_STATISTICS.DATE IS '일시';

-- 평균방문횟수
COMMENT ON COLUMN CHILD_ZONE_VISITING_DAY_STATISTICS.COUNT IS '평균방문횟수';

-- 자녀놀이구역방문일통계 기본키
CREATE UNIQUE INDEX PK_CHILD_ZONE_VISITING_DAY_STATISTICS
	ON CHILD_ZONE_VISITING_DAY_STATISTICS
	( -- 자녀놀이구역방문일통계
		ZONE_ID ASC, -- 놀이구역ID
		DATE ASC -- 일시
	)
;
-- 자녀놀이구역방문일통계
ALTER TABLE CHILD_ZONE_VISITING_DAY_STATISTICS
	ADD CONSTRAINT PK_CHILD_ZONE_VISITING_DAY_STATISTICS
		 -- 자녀놀이구역방문일통계 기본키
	PRIMARY KEY 
	USING INDEX PK_CHILD_ZONE_VISITING_DAY_STATISTICS;

-- 자녀놀이구역방문일통계 기본키
COMMENT ON CONSTRAINT PK_CHILD_ZONE_VISITING_DAY_STATISTICS ON CHILD_ZONE_VISITING_DAY_STATISTICS IS '자녀놀이구역방문일통계 기본키';

-- 자녀심박수시통계
CREATE TABLE CHILD_PULSE_HOUR_STATISTICS
(
	CHILD_ID varchar(50) NOT NULL, -- 자녀ID
	DATE     timestamp   NOT NULL, -- 일시
	AVERAGE  integer     NULL,     -- 평균심박수
	MAXIMUM  integer     NULL,     -- 최대심박수
	MINIMUM  integer     NULL      -- 최소심박수
);

-- 자녀심박수시통계
COMMENT ON TABLE CHILD_PULSE_HOUR_STATISTICS IS '자녀심박수시통계';

-- 자녀ID
COMMENT ON COLUMN CHILD_PULSE_HOUR_STATISTICS.CHILD_ID IS '자녀ID';

-- 일시
COMMENT ON COLUMN CHILD_PULSE_HOUR_STATISTICS.DATE IS '일시';

-- 평균심박수
COMMENT ON COLUMN CHILD_PULSE_HOUR_STATISTICS.AVERAGE IS '평균심박수';

-- 최대심박수
COMMENT ON COLUMN CHILD_PULSE_HOUR_STATISTICS.MAXIMUM IS '최대심박수';

-- 최소심박수
COMMENT ON COLUMN CHILD_PULSE_HOUR_STATISTICS.MINIMUM IS '최소심박수';

-- 자녀심박수시통계 기본키
CREATE UNIQUE INDEX PK_CHILD_PULSE_HOUR_STATISTICS
	ON CHILD_PULSE_HOUR_STATISTICS
	( -- 자녀심박수시통계
		CHILD_ID ASC, -- 자녀ID
		DATE ASC -- 일시
	)
;
-- 자녀심박수시통계
ALTER TABLE CHILD_PULSE_HOUR_STATISTICS
	ADD CONSTRAINT PK_CHILD_PULSE_HOUR_STATISTICS
		 -- 자녀심박수시통계 기본키
	PRIMARY KEY 
	USING INDEX PK_CHILD_PULSE_HOUR_STATISTICS;

-- 자녀심박수시통계 기본키
COMMENT ON CONSTRAINT PK_CHILD_PULSE_HOUR_STATISTICS ON CHILD_PULSE_HOUR_STATISTICS IS '자녀심박수시통계 기본키';

-- 놀이구역방문시통계
CREATE TABLE ZONE_VISITING_HOUR_STATISTICS
(
	ZONE_ID varchar(50) NOT NULL, -- 놀이구역ID
	AGE     integer     NOT NULL, -- 나이
	DATE    timestamp   NOT NULL, -- 일시
	AVERAGE integer     NOT NULL, -- 평균방문횟수
	MINIMUM integer     NULL,     -- 최소방문횟수
	MAXIMUM integer     NULL      -- 최대방문횟수
);

-- 놀이구역방문시통계
COMMENT ON TABLE ZONE_VISITING_HOUR_STATISTICS IS '놀이구역방문시통계';

-- 놀이구역ID
COMMENT ON COLUMN ZONE_VISITING_HOUR_STATISTICS.ZONE_ID IS '놀이구역ID';

-- 나이
COMMENT ON COLUMN ZONE_VISITING_HOUR_STATISTICS.AGE IS '나이';

-- 일시
COMMENT ON COLUMN ZONE_VISITING_HOUR_STATISTICS.DATE IS '일시';

-- 평균방문횟수
